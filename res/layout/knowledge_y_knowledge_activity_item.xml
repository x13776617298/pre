<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical" >

    <FrameLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" >

        <LinearLayout
            android:id="@+id/information_layout_other"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_gravity="center"
            android:background="@drawable/y_list_selector_background"
            android:orientation="horizontal"
            android:paddingTop="30dip" >

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingTop="8dip" >

                <TextView
                    android:id="@+id/text_today"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/y_icon_today"
                    android:text="今天"
                    android:textColor="@color/white"
                    android:textSize="12sp" android:gravity="center|left" android:layout_marginTop="6dp"/>

                <TextView
                    android:id="@+id/text_pregnancy_week"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/green"
                    android:textSize="12sp" android:paddingLeft="4dip" android:layout_below="@+id/text_pregnancy_date"/>
                <TextView
                    android:id="@+id/text_pregnancy_date"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/green"
                    android:textSize="12sp" android:paddingLeft="4dip" android:layout_below="@+id/text_today"/>

                

                
            </RelativeLayout>
            <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingTop="10dp" android:paddingLeft="5dip">
                <RelativeLayout android:layout_width="wrap_content" android:layout_height="wrap_content" android:background="@drawable/y_layout_selector_background"
                    android:id="@+id/rl_1" android:visibility="gone">
                    <TextView
                        android:id="@+id/txt_title_1"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/red"/>
                    <TextView
                        android:id="@+id/txt_subtitle_1"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/black"
                        android:textSize="16sp"
                        android:textStyle="bold" android:layout_marginBottom="20dp"
                        android:layout_below="@id/txt_title_1"/>
                    <ImageView
                                android:id="@+id/content_image_1"
                                android:layout_width="60dp"
                                android:layout_height="60dp" android:background="@drawable/y_img_bkg"
                                android:layout_below="@id/txt_subtitle_1"/>
                    
                    <ImageView
                            android:id="@+id/ic_arrow_1" 
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true" android:layout_alignParentRight="true" 
                            android:background="#00000000"
                            android:contentDescription="@string/app_name"
                            android:src="@drawable/ic_arrow" 
                            android:paddingRight="3dp"/>
                    <TextView
                                android:id="@+id/txt_content_1"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:textColor="#5a5858" 
                                android:paddingLeft="4dip" android:layout_toRightOf="@id/content_image_1" android:layout_toLeftOf="@id/ic_arrow_1"
                                android:paddingTop="45dip"/>
                    
                </RelativeLayout>
				<ImageView
                        android:id="@+id/img_knowledge_line1"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:src="@drawable/y_line_bkg"
                        android:visibility="gone" />
				<RelativeLayout android:layout_width="wrap_content" android:layout_height="wrap_content" android:background="@drawable/y_layout_selector_background"
                    android:id="@+id/rl_2" android:visibility="gone" android:paddingTop="10dp" android:paddingBottom="10dp">
				<TextView
                        android:id="@+id/txt_title_2"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/red"
                        android:visibility="gone" />
				
				<ImageView
                            android:id="@+id/img_knowledge_2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true" android:layout_centerVertical="true"
                            android:background="#00000000"
                            android:clickable="true"
                            android:contentDescription="@string/app_name"
                            android:focusable="false"
                            android:focusableInTouchMode="false"
                            android:paddingRight="3dp"
                            android:src="@drawable/ic_arrow" />
				<TextView android:id="@+id/tv_knowledge_2" android:layout_width="fill_parent" android:layout_height="wrap_content"  
				    android:textColor="@color/black" android:layout_below="@id/txt_title_2"
				    android:layout_toLeftOf="@id/img_knowledge_2"/>
				</RelativeLayout>
				<ImageView
                        android:id="@+id/img_knowledge_line2"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:src="@drawable/y_line_bkg"
                        android:visibility="gone" />
				<RelativeLayout android:layout_width="wrap_content" android:layout_height="wrap_content" android:background="@drawable/y_layout_selector_background"
                    android:id="@+id/rl_3" android:visibility="gone">
                    <TextView
                        android:id="@+id/txt_title_3"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/red" />
                    <TextView
                        android:id="@+id/txt_subtitle_3"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/black"
                        android:textSize="16sp"
                        android:textStyle="bold"
                        android:layout_below="@id/txt_title_3"/>
                    <ImageView
                                android:id="@+id/content_image_3"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content" android:background="@drawable/y_img_bkg"
                                android:layout_below="@id/txt_subtitle_3"/>
                    
                    <ImageView
                            android:id="@+id/ic_arrow_3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true" android:layout_alignParentRight="true" 
                            android:background="#00000000"
                            android:contentDescription="@string/app_name"
                            android:src="@drawable/ic_arrow" 
                            android:paddingRight="3dp"/>
                    <TextView	
                                android:id="@+id/txt_content_3"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:maxLines="5"
                                android:textColor="#5a5858" 
                                android:paddingLeft="4dip" android:layout_toRightOf="@id/content_image_3" android:layout_toLeftOf="@id/ic_arrow_3"
                                android:paddingTop="45dip"/>
                    
                </RelativeLayout>
				<ImageView
                        android:id="@+id/img_knowledge_line3"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:src="@drawable/y_line_bkg"
                        android:visibility="gone" />
				<RelativeLayout android:layout_width="wrap_content" android:layout_height="wrap_content" android:background="@drawable/y_layout_selector_background"
                    android:id="@+id/rl_4" android:visibility="gone" android:paddingTop="10dp" android:paddingBottom="10dp">
				<TextView
                        android:id="@+id/txt_title_4"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/red"
                        android:visibility="gone" />
				
				<ImageView
                            android:id="@+id/img_knowledge_4"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true" android:layout_centerVertical="true"
                            android:background="#00000000"
                            android:clickable="true"
                            android:contentDescription="@string/app_name"
                            android:focusable="false"
                            android:focusableInTouchMode="false"
                            android:paddingRight="3dp"
                            android:src="@drawable/ic_arrow" />
				<TextView android:id="@+id/tv_knowledge_4" android:layout_width="fill_parent" android:layout_height="wrap_content"  
				    android:textColor="@color/black" android:layout_below="@id/txt_title_4"
				    android:layout_toLeftOf="@id/img_knowledge_4"/>
				</RelativeLayout>
					
					  <ImageView
                        android:id="@+id/line0"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:src="@drawable/y_line_bkg"
                        android:visibility="gone" />
                    <RelativeLayout
                        android:id="@+id/ll0"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:visibility="gone"
                        android:paddingTop="10dp"
                        android:paddingBottom="10dp" android:background="@drawable/y_layout_selector_background">

                        <CheckBox
                            android:id="@+id/layout_remind_checkbox0"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/y_information_checkbox_selector"
                            android:button="@null"
                            android:focusable="false"
                            android:focusableInTouchMode="false" android:layout_alignParentLeft="true"
                            android:layout_centerVertical="true"/>

                        <TextView
                            android:id="@+id/layout_remind_textview0"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_toRightOf="@id/layout_remind_checkbox0"
                            android:paddingLeft="10dp"
                            android:paddingRight="10dp"
                            android:textColor="@color/black" />

                        <ImageView
                            android:id="@+id/ic_arrow_bottom0"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true" android:layout_centerVertical="true"
                            android:background="#00000000"
                            android:clickable="true"
                            android:contentDescription="@string/app_name"
                            android:focusable="false"
                            android:focusableInTouchMode="false"
                            android:src="@drawable/ic_arrow" android:paddingRight="3dp"/>
                    </RelativeLayout>

                    <ImageView
                        android:id="@+id/line1"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:src="@drawable/y_line_bkg"
                        android:visibility="gone"/>

                    <RelativeLayout
                        android:id="@+id/ll1"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:visibility="gone" android:background="@drawable/y_layout_selector_background"
                        android:paddingTop="10dp"
                        android:paddingBottom="10dp">

                        <CheckBox
                            android:id="@+id/layout_remind_checkbox1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/y_information_checkbox_selector"
                            android:button="@null"
                            android:focusable="false"
                            android:focusableInTouchMode="false" android:layout_alignParentLeft="true"
                            android:layout_centerVertical="true"/>

                        <TextView
                            android:id="@+id/layout_remind_textview1"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"  android:layout_centerVertical="true"
                            android:paddingLeft="10dp" android:paddingRight="10dp"
                            android:textColor="@color/black" android:layout_toRightOf="@id/layout_remind_checkbox1"/>

                        <ImageView
                            android:id="@+id/ic_arrow_bottom1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
							android:layout_alignParentRight="true" android:layout_centerVertical="true"
                            android:background="#00000000"
                            android:clickable="true"
                            android:contentDescription="@string/app_name"
                            android:focusable="false"
                            android:focusableInTouchMode="false"
                            android:paddingRight="3dp"
                            android:src="@drawable/ic_arrow" />
                    </RelativeLayout>

                    <ImageView
                        android:id="@+id/line2"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:src="@drawable/y_line_bkg"
                        android:visibility="gone"/>

                    <RelativeLayout
                        android:id="@+id/ll2"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:visibility="gone" android:background="@drawable/y_layout_selector_background"
                        android:paddingTop="10dp"
                        android:paddingBottom="10dp">

                        <CheckBox
                            android:id="@+id/layout_remind_checkbox2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/y_information_checkbox_selector"
                            android:button="@null"
                            android:focusable="false"
                            android:focusableInTouchMode="false" android:layout_alignParentLeft="true"
                            android:layout_centerVertical="true"/>

                        <TextView
                            android:id="@+id/layout_remind_textview2"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"  android:layout_centerVertical="true"
                            android:paddingLeft="10dp" android:paddingRight="10dp"
                            android:textColor="@color/black" android:layout_toRightOf="@id/layout_remind_checkbox2"/>

                        <ImageView
                            android:id="@+id/ic_arrow_bottom2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
							android:layout_alignParentRight="true" android:layout_centerVertical="true"
                            android:background="#00000000"
                            android:clickable="true"
                            android:contentDescription="@string/app_name"
                            android:focusable="false"
                            android:focusableInTouchMode="false"
                            android:paddingRight="3dp"
                            android:src="@drawable/ic_arrow" />
                    </RelativeLayout>
                </LinearLayout>
        </LinearLayout>
        <TextView
            android:id="@+id/information_header"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="top"
            android:background="@drawable/y_section_bar_bkg"
            android:textColor="@color/white"
            android:textSize="13sp"
            android:textStyle="bold" >
        </TextView>
    </FrameLayout>
     <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/y_driver" android:layout_marginLeft="0dip" android:layout_marginRight="0dip">
     </LinearLayout>

</LinearLayout>